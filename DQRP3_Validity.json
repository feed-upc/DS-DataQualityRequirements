{
    "@context": { "odrl:operator": { "@type": "@id" },
        "@base": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "odrl": "http://www.w3.org/ns/odrl/2/",
        "dcat": "http://www.w3.org/ns/dcat#",
        "dqv": "http://www.w3.org/ns/dqv#",
        "tb": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/tbox#",
        "ab": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/abox#",
        "prov": "http://www.w3.org/ns/prov#"
    },
    "@graph": [
        {
            "@id": "ab:DQRP3",
            "@type": "dqv:QualityPolicy",
            "rdfs:label": "DQRP3 - Validity Requirement",
            "rdfs:comment": "Data Quality Requirement Pattern 3: Values of an attribute must be in a controlled vocabulary (ISO/IEC 25012 Validity)",
            "tb:derivedFrom": "DQRP3",
            "tb:qualityDimension": "Validity",
            "odrl:duty": [
                {
                    "@id": "ab:DQRP3_Duty",
                    "@type": "odrl:Duty",
                    "rdfs:comment": "Gender attribute values must be in the controlled vocabulary {M, F}",
                    "odrl:target": {
                        "@id": "ab:Sex"
                    },
                    "odrl:constraint": [
                        {
                            "@id": "ab:DQRP3_Constraint",
                            "@type": "odrl:Constraint",
                            "rdfs:comment": "Values must be part of controlled vocabulary",
                            "odrl:leftOperand": {
                                "@id": "ab:CheckValidity"
                            },
                            "odrl:operator": "odrl:isPartOf",
                            "odrl:rightOperand": {
                                "@id": "ab:GenderVocabulary"
                            }
                        }
                    ]
                }
            ]
        },
        {
            "@id": "ab:GenderVocabulary",
            "@type": "tb:ControlledVocabulary",
            "rdfs:label": "Gender Controlled Vocabulary",
            "tb:allowedValues": [
                "M",
                "F"
            ]
        }
    ]
}
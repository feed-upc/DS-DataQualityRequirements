{
    "@context": {
        "odrl:operator": {
            "@type": "@id"
        },
        "@base": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "odrl": "http://www.w3.org/ns/odrl/2/",
        "dcat": "http://www.w3.org/ns/dcat#",
        "dqv": "http://www.w3.org/ns/dqv#",
        "tb": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/tbox#",
        "ab": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/abox#",
        "prov": "http://www.w3.org/ns/prov#",
        "qudt": "http://qudt.org/schema/qudt/"
    },
    "@graph": [
        {
            "@id": "ab:DQRP2",
            "@type": "dqv:QualityPolicy",
            "rdfs:label": "DQRP2 - Completeness Requirement",
            "rdfs:comment": "Data Quality Requirement Pattern 2: Completeness check. The Subject (Patient ID) must be 100% complete (no nulls).",
            "tb:derivedFrom": "DQRP2",
            "tb:qualityDimension": "Completeness",
            "tb:sourceEntity": {
                "@id": "ab:DataSpaceGovernanceAuthority"
            },
            "odrl:permission": [
                {
                    "@id": "ab:DQRP2_Permission",
                    "@type": "odrl:Permission",
                    "rdfs:comment": "Data may only be distributed if Subject completeness is 100%",
                    "odrl:action": "odrl:distribute",
                    "odrl:assigner": {
                        "@id": "ab:DataSpaceGovernanceAuthority"
                    },
                    "odrl:assignee": {
                        "@id": "ab:DataProvider"
                    },
                    "odrl:target": {
                        "@id": "ab:Subject"
                    },
                    "odrl:constraint": [
                        {
                            "@id": "ab:DQRP2_Constraint",
                            "@type": "odrl:Constraint",
                            "rdfs:comment": "Completeness must be >= 100",
                            "odrl:leftOperand": {
                                "@id": "ab:CompletenessMeasurement",
                                "@type": "dqv:QualityMeasurement"
                            },
                            "odrl:operator": "odrl:gteq",
                            "odrl:rightOperand": {
                                "@value": "100",
                                "@type": "xsd:integer"
                            },
                            "odrl:unit": {
                                "@id": "qudt:PERCENT"
                            }
                        }
                    ]
                }
            ]
        },
        {
            "@id": "ab:CompletenessMeasurement",
            "@type": "dqv:Metric",
            "rdfs:label": "Completeness Measurement",
            "rdfs:comment": "Measures the completeness of an attribute (percentage of non-null values)",
            "dqv:isMeasurementOf": {
                "@id": "ab:qM"
            }
        },
        {
            "@id": "ab:Completeness",
            "@type": "dqv:Metric",
            "rdfs:label": "Completeness Metric (Abstract)",
            "dqv:inDimension": {
                "@id": "ab:CompletenessDimension",
                "@type": "dqv:Dimension"
            }
        },
        {
            "@id": "ab:CompletenessDimension",
            "@type": "dqv:Dimension",
            "rdfs:label": "Completeness (ISO/IEC 25012)"
        },
        {
            "@id": "ab:Subject",
            "@type": "odrl:Asset",
            "odrl:partOf": {
                "@id": "ab:EHDS_Patient_Summary"
            }
        }
    ]
}
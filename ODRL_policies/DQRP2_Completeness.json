    "@graph": [
        {
            "@id": "ab:DQRP2",
            "@type": "dqv:QualityPolicy",
            "rdfs:label": "DQRP2 - Completeness Requirement",
            "rdfs:comment": "Data Quality Requirement Pattern 2: Values of an attribute must be mandatory (ISO/IEC 25012 Completeness)",
            "tb:derivedFrom": "DQRP2",
            "tb:qualityDimension": "Completeness",
            "odrl:duty": [
                {
                    "@id": "ab:DQRP2_Duty",
                    "@type": "odrl:Duty",
                    "rdfs:comment": "Age_at_scan_years attribute must contain at least 99% non-empty values",
                    "odrl:target": {
                        "@id": "ab:Age"
                    },
                    "odrl:constraint": [
                        {
                            "@id": "ab:DQRP2_Constraint",
                            "@type": "odrl:Constraint",
                            "rdfs:comment": "Completeness must be at least 99%",
                            "odrl:leftOperand": {
                                "@id": "ab:NonNullValues"
                            },
                            "odrl:operator": "odrl:gteq",
                            "odrl:rightOperand": {
                                "@value": "99",
                                "@type": "xsd:decimal"
                            }
                        }
                    ]
                }
            ]
        }
    ]

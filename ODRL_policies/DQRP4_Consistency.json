{
    "@context": {
        "odrl:operator": {
            "@type": "@id"
        },
        "@base": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "odrl": "http://www.w3.org/ns/odrl/2/",
        "dcat": "http://www.w3.org/ns/dcat#",
        "dqv": "http://www.w3.org/ns/dqv#",
        "tb": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/tbox#",
        "ab": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/abox#",
        "prov": "http://www.w3.org/ns/prov#",
        "qudt": "http://qudt.org/schema/qudt/"
    },
    "@graph": [
        {
            "@id": "ab:DQRP4",
            "@type": "dqv:QualityPolicy",
            "rdfs:label": "DQRP4 - Consistency Requirement",
            "rdfs:comment": "Data Quality Requirement Pattern 4: Consistency check. If gender is Male, pregnancyHistory must be empty/NULL.",
            "tb:derivedFrom": "DQRP4",
            "tb:qualityDimension": "Consistency",
            "tb:sourceEntity": {
                "@id": "ab:DataSpaceGovernanceAuthority"
            },
            "odrl:permission": [
                {
                    "@id": "ab:DQRP4_Permission",
                    "@type": "odrl:Permission",
                    "rdfs:comment": "Data may only be distributed if males have no pregnancy history",
                    "odrl:action": "odrl:distribute",
                    "odrl:assigner": {
                        "@id": "ab:DataSpaceGovernanceAuthority"
                    },
                    "odrl:assignee": {
                        "@id": "ab:DataProvider"
                    },
                    "odrl:target": {
                        "@id": "ab:pregnancyHistory"
                    },
                    "odrl:constraint": [
                        {
                            "@id": "ab:DQRP4_Constraint",
                            "@type": "odrl:Constraint",
                            "rdfs:comment": "Consistency condition: if gender is M, pregnancyHistory must be NULL",
                            "odrl:leftOperand": {
                                "@id": "ab:ConsistencyMeasurement",
                                "@type": "dqv:QualityMeasurement"
                            },
                            "odrl:operator": "odrl:eq",
                            "odrl:rightOperand": {
                                "@value": "100",
                                "@type": "xsd:integer"
                            },
                            "odrl:unit": {
                                "@id": "qudt:PERCENT"
                            }
                        }
                    ]
                }
            ]
        },
        {
            "@id": "ab:ConsistencyMeasurement",
            "@type": "dqv:Metric",
            "rdfs:label": "Consistency Measurement",
            "rdfs:comment": "Measures the logical consistency between two conditionally dependent attributes (e.g. gender and pregnancyHistory)",
            "dqv:isMeasurementOf": {
                "@id": "ab:qM_Consistency"
            }
        },
        {
            "@id": "ab:Consistency",
            "@type": "dqv:Metric",
            "rdfs:label": "Consistency Metric (Abstract)",
            "dqv:inDimension": {
                "@id": "ab:ConsistencyDimension",
                "@type": "dqv:Dimension"
            }
        },
        {
            "@id": "ab:ConsistencyDimension",
            "@type": "dqv:Dimension",
            "rdfs:label": "Consistency (ISO/IEC 25012)"
        },
        {
            "@id": "ab:pregnancyHistory",
            "@type": "odrl:Asset",
            "odrl:partOf": {
                "@id": "ab:EHDS_Patient_Summary"
            }
        }
    ]
}
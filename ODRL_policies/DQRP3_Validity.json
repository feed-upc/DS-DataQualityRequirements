{
    "@context": {
        "odrl:operator": {
            "@type": "@id"
        },
        "@base": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "odrl": "http://www.w3.org/ns/odrl/2/",
        "dcat": "http://www.w3.org/ns/dcat#",
        "dqv": "http://www.w3.org/ns/dqv#",
        "tb": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/tbox#",
        "ab": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/abox#",
        "prov": "http://www.w3.org/ns/prov#",
        "qudt": "http://qudt.org/schema/qudt/"
    },
    "@graph": [
        {
            "@id": "ab:DQRP3",
            "@type": "dqv:QualityPolicy",
            "rdfs:label": "DQRP3 - Validity Requirement",
            "rdfs:comment": "Data Quality Requirement Pattern 3: Values of an attribute must follow a specific format. The countryOfAffiliation must follow the ISO 3166 standard.",
            "tb:derivedFrom": "DQRP3",
            "tb:qualityDimension": "Validity",
            "tb:sourceEntity": {
                "@id": "ab:DataSpaceGovernanceAuthority"
            },
            "odrl:permission": [
                {
                    "@id": "ab:DQRP3_Permission",
                    "@type": "odrl:Permission",
                    "rdfs:comment": "Data may only be distributed if AffiliationCountry values adhere to ISO 3166 strictly",
                    "odrl:action": "odrl:distribute",
                    "odrl:assigner": {
                        "@id": "ab:DataSpaceGovernanceAuthority"
                    },
                    "odrl:assignee": {
                        "@id": "ab:DataProvider"
                    },
                    "odrl:target": {
                        "@id": "ab:AffiliationCountry"
                    },
                    "odrl:constraint": [
                        {
                            "@id": "ab:DQRP3_Constraint",
                            "@type": "odrl:Constraint",
                            "rdfs:comment": "Validity adherence must be exactly 100",
                            "odrl:leftOperand": {
                                "@id": "ab:ValidityMeasurement",
                                "@type": "dqv:QualityMeasurement"
                            },
                            "odrl:operator": "odrl:eq",
                            "odrl:rightOperand": {
                                "@value": "100",
                                "@type": "xsd:integer"
                            },
                            "odrl:unit": {
                                "@id": "qudt:PERCENT"
                            }
                        }
                    ]
                }
            ]
        },
        {
            "@id": "ab:ValidityMeasurement",
            "@type": "dqv:Metric",
            "rdfs:label": "Validity ISO3166 Measurement",
            "rdfs:comment": "Measures the adherence of a string attribute to ISO 3166-1 alpha-2 standards",
            "dqv:isMeasurementOf": {
                "@id": "ab:qM_Validity"
            }
        },
        {
            "@id": "ab:Validity",
            "@type": "dqv:Metric",
            "rdfs:label": "Validity Metric (Abstract)",
            "dqv:inDimension": {
                "@id": "ab:ValidityDimension",
                "@type": "dqv:Dimension"
            }
        },
        {
            "@id": "ab:ValidityDimension",
            "@type": "dqv:Dimension",
            "rdfs:label": "Validity (DAMA-DMBOK)"
        },
        {
            "@id": "ab:AffiliationCountry",
            "@type": "odrl:Asset",
            "odrl:partOf": {
                "@id": "ab:EHDS_Patient_Summary"
            }
        }
    ]
}
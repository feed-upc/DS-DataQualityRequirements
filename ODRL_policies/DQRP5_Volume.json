{
    "@context": {
        "odrl:operator": {
            "@type": "@id"
        },
        "@base": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "odrl": "http://www.w3.org/ns/odrl/2/",
        "dcat": "http://www.w3.org/ns/dcat#",
        "dqv": "http://www.w3.org/ns/dqv#",
        "tb": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/tbox#",
        "ab": "http://www.semanticweb.org/acraf/ontologies/2024/healthmesh/abox#",
        "prov": "http://www.w3.org/ns/prov#",
        "qudt": "http://qudt.org/schema/qudt/"
    },
    "@graph": [
        {
            "@id": "ab:DQRP5",
            "@type": "dqv:QualityPolicy",
            "rdfs:label": "DQRP5 - Volume Requirement",
            "rdfs:comment": "Data Quality Requirement Pattern 5: In the entity, the number of instances must be at least 2000.",
            "tb:derivedFrom": "DQRP5",
            "tb:qualityDimension": "Volume",
            "tb:sourceEntity": {
                "@id": "ab:DataSpaceGovernanceAuthority"
            },
            "odrl:permission": [
                {
                    "@id": "ab:DQRP5_Permission",
                    "@type": "odrl:Permission",
                    "rdfs:comment": "Data may only be distributed if it has at least 2000 instances",
                    "odrl:action": "odrl:distribute",
                    "odrl:assigner": {
                        "@id": "ab:DataSpaceGovernanceAuthority"
                    },
                    "odrl:assignee": {
                        "@id": "ab:DataProvider"
                    },
                    "odrl:target": {
                        "@id": "ab:DatasetLevel"
                    },
                    "odrl:constraint": [
                        {
                            "@id": "ab:DQRP5_Constraint",
                            "@type": "odrl:Constraint",
                            "rdfs:comment": "Dataset must have at least 2000 instances",
                            "odrl:leftOperand": {
                                "@id": "ab:RowCountMeasurement",
                                "@type": "dqv:QualityMeasurement"
                            },
                            "odrl:operator": "odrl:gteq",
                            "odrl:rightOperand": {
                                "@value": "2000",
                                "@type": "xsd:integer"
                            },
                            "odrl:unit": {
                                "@id": "qudt:NUM"
                            }
                        }
                    ]
                }
            ]
        },
        {
            "@id": "ab:RowCountMeasurement",
            "@type": "dqv:Metric",
            "rdfs:label": "Row Count Measurement",
            "rdfs:comment": "Measures the number of rows/instances in a dataset",
            "dqv:isMeasurementOf": {
                "@id": "ab:qM_RowCount"
            }
        },
        {
            "@id": "ab:Volume",
            "@type": "dqv:Metric",
            "rdfs:label": "Volume Metric (Abstract)",
            "dqv:inDimension": {
                "@id": "ab:VolumeDimension",
                "@type": "dqv:Dimension"
            }
        },
        {
            "@id": "ab:VolumeDimension",
            "@type": "dqv:Dimension",
            "rdfs:label": "Volume (Number of Instances)"
        },
        {
            "@id": "ab:DatasetLevel",
            "@type": "odrl:Asset",
            "odrl:partOf": {
                "@id": "ab:EHDS_Patient_Summary"
            }
        }
    ]
}